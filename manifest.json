{
  "manifest_version": 3,
  "name": "Pull Request Manager for GitHub and Bitbucket",
  "version": "0.0.1",
  "description": "View and manage your pull requests from GitHub and Bitbucket.",
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png",
    "512" : "icons/512.png"
  }
}